import turtle
wn = turtle.Screen()
pirate = turtle.Turtle()
pirate.shape("turtle")

pirate.penup()
pirate.forward(60)

pirate.pendown()
for a in [160, -43, 270, -97, -43, 200, -940, 17, -86]:
    pirate.left(a)
    pirate.forward(100)
    print('current heading is:', pirate.heading())
    
wn.exitonclick()
